<h1>RecipesController#index</h1>
<p>Find me in app/views/recipes_controller/index.html.erb</p>
<table border="1">
  <tr>
  	<th>Image</th>
  	<th>Title</th>
  	<th>Rank</th>
  </tr>
  <% @recipes.each do |recipe| %>
    <tr class=<%= cycle('even', 'odd') %>>
      <td><%= link_to image_tag(recipe["image_url"], size: "100x100").html_safe, "#{recipe["source_url"]}"%></td>
      <td><%= link_to sanitize(recipe["title"]), "#{recipe["source_url"]}".html_safe %></td>
      <td><%= recipe["social_rank"] %></td>
    </tr>
  <% end %>
</table>
<p>Powered By Food2Fork.com</p>
